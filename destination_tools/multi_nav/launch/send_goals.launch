<?xml version="1.0"?>
<launch>
  <!-- BEGIN PIONEER 1 -->
  <group ns="pioneer1">
    <param name="poz_x" value="-2" />
    <param name="poz_y" value="0" />
    <param name="poz_theta" value="90" />
    <node pkg="multi_nav" type="send_multiple_goal" respawn="false" name="send_multiple_goal" output="screen" args="1"/>
  </group>

  <!-- BEGIN PIONEER 2 -->
  <group ns="pioneer2">
    <param name="poz_x" value="0" />
    <param name="poz_y" value="-2" />
    <param name="poz_theta" value="0" />
    <node pkg="multi_nav" type="send_multiple_goal" respawn="false" name="send_multiple_goal" output="screen" args="2"/>
  </group>
</launch>

