<?xml version="1.0"?>
<launch>
  <arg name="robot_name"/>
    
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find robotnav_try)/config/costmap_common_params_kinect_$(arg robot_name).yaml" command="load" ns="global_costmap" /> 
    
    <rosparam file="$(find robotnav_try)/config/costmap_common_params_kinect_$(arg robot_name).yaml" command="load" ns="local_costmap" /> 
    <rosparam file="$(find robotnav_try)/config/local_costmap_params.yaml" command="load" />
    
    <rosparam file="$(find robotnav_try)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robotnav_try)/config/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
