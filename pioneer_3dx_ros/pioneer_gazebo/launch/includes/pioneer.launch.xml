<launch>
  <arg name="base"/>
  <arg name="3d_sensor"/>

  <arg name="urdf_file" default="$(find xacro)/xacro --inorder '$(find pioneer_description)/urdf/$(arg base)_$(arg 3d_sensor).urdf.xacro'"/> <!--maybe .urdf.xacro?-->
  <param name="robot_description" command="$(arg urdf_file)"/>
  <!--gazebo model spawner-->
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
	args="-x 0 -y 0 -z 0 -Y 1.57 -urdf -model pioneer -param robot_description"/>
</launch>
