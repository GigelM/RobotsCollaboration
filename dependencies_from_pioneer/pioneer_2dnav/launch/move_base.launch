<?xml version="1.0"?>
<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find pioneer_2dnav)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
