    <rosparam file="$(find pioneer_2dnav)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find pioneer_2dnav)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find pioneer_2dnav)/config/base_local_planner_params.yaml" command="load" />
    <!-- NOTE: Use move_base_global.launch for globally qualified map topic -->
    
